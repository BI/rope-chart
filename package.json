{
  "name": "rope-chart",
  "version": "0.1.0",
  "description": "provides a simplified visualization of where a particular value lies within a group.",
  "repository": "BI/rope-chart",
  "main": "dist/ropeChart.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config webpack.demo.config.js --content-base demo/",
    "dist": "NODE_ENV=production webpack --config webpack.dist.config.js",
    "doc": "jsdoc2md --template README.hbs src/ropeChart.js > README.md",
    "test": "jest"
  },
  "tags": [
    "d3",
    "chart",
    "visualization",
    "dataviz"
  ],
  "author": "Barquin International",
  "license": "ISC",
  "devDependencies": {
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "jest-cli": "^12.1.1",
    "jsdoc-to-markdown": "^1.3.6",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "d3-tip": "git://github.com/BI/d3-tip"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(scss|css)$": "<rootDir>/__mocks__/styleMock.js"
